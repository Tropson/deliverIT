@model IEnumerable<WebClientMVC.Models.ApplicationModel>
@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

<h2>List</h2>
<div class="row">
    <div class="col-lg-9 col-md-9 col-sm-9">
        <table class="table">
            <thead>
            <th>Cpr</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Phone Number</th>
            <th>Email</th>
            <th>Address</th>
            <th>ZipCode</th>
            <th>City</th>
            </thead>
            <tbody>
                @foreach (var app in Model)
                {
                    <tr>
                        <td>@app.Cpr</td>
                        <td>@app.FirstName</td>
                        <td>@app.LastName</td>
                        <td>@app.PhoneNumber</td>
                        <td>@app.Email</td>
                        <td>@app.Address</td>
                        <td>@app.ZipCode</td>
                        <td>@app.City</td>
                        <td>
                            @Html.ActionLink("Details", "Details", "ApplicationModel", new { app.Cpr }, new { @class = "btn btn-default col-md-3" })
                    </td>
                </tr>

            }

            </tbody>

        </table>

    </div>
</div>